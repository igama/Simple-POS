<h1>Listing employee_details</h1>

<table>
  <tr>
    <th>User</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Shop</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @employee_details.each do |employee_detail| %>
  <tr>
    <td><%= employee_detail.user_id %></td>
    <td><%= employee_detail.first_name %></td>
    <td><%= employee_detail.last_name %></td>
    <td><%= employee_detail.shop_id %></td>
    <td><%= link_to 'Show', employee_detail %></td>
    <td><%= link_to 'Edit', edit_employee_detail_path(employee_detail) %></td>
    <td><%= link_to 'Destroy', employee_detail, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

